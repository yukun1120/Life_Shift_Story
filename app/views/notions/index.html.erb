<div class="home">
  
  <%= render "shared/side"%>

  <div class="home-main">

    <%= render "shared/header"%>

    <div class="notion-main">

      <div class="notions">
        <% @notions.each do |notion| %>
        <div class="notion-list">
          <h3 class='notion-title' data-id="<%= notion.id %>">
            <%= notion.title %>
          </h3>
        </div>
        <% end %>
        <div class='notion-content' id="notion-content">
        </div>
      </div>

    </div>

    <%= render "shared/footer"%>
    
  </div>

</div>